{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "hcltheme2/image-content",
  "title": "Image with Title",
  "category": "media",
  "icon": "format-image",
  "description": "A block that displays an image with a title overlay.",
  "attributes": {
    "imageUrl": {
      "type": "string",
      "default": ""
    },
    "title": {
      "type": "string",
      "default": "Your Title Here"
    }
  },
  "editorScript": "file:./index.js",
  "textdomain": "hcltheme2",
  "style": "file:./style.css",
  "editorStyle": "file:./editor.css"
}
